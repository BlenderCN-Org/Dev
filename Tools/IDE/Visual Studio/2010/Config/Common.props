			<PreprocessorDefinitions>WIN32;_WINDOWS;_CONSOLE;%(PreprocessorDefinitions)</PreprocessorDefinitions>
			<RuntimeTypeInfo>false</RuntimeTypeInfo>
			<WarningLevel>Level3</WarningLevel>
