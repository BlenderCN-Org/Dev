	</ItemDefinitionGroup>
	<ItemGroup />
	<Import Condition="'$(Platform)'=='Win32'" Project="Win32.props" />
	<Import Condition="'$(Platform)'=='x64'" Project="Win64.props" />
	<Import Condition="'$(Configuration)'=='Debug'" Project="Debug.props" />
	<Import Condition="'$(Configuration)'=='Release'" Project="Release.props" />
</Project>
